{
    "targets": {
        "release": {
            "outFile": "build/release.wasm"
        }
    },
    "options": {
        "sourceMap": false,
        "optimizeLevel": 3,
        "shrinkLevel": 1,
        "converge": true,
        "noAssert": false,
        "enable": [
            "sign-extension",
            "mutable-globals",
            "nontrapping-f2i",
            "bulk-memory",
            "simd",
            "reference-types",
            "multi-value"
        ],
        "runtime": "stub",
        "memoryBase": 1024,
        "lowMemoryLimit": 65536,
        "initialMemory": 4,
        "maximumMemory": 16,
        "debug": false,
        "trapMode": "allow",
        "transform": "@btc-vision/opnet-transform"
    }
}
